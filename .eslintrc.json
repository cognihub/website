{
  "extends": [
    "airbnb",
    "next/core-web-vitals"
  ],
  "rules": {
    "indent": ["error", 4],
    "react/jsx-indent" : ["error", 4],
    "react/jsx-one-expression-per-line": "off",
    "react/jsx-indent-props": ["error", 4],
    "semi": "off",
    "comma-dangle": "off",
    "jsx-quotes": [2, "prefer-single"],
    "import/no-unresolved": 0,
    "quote-props": "off",
    "import/extensions": "off",
    "prefer-template": "off",
    "max-len": "warn"
  }
}
